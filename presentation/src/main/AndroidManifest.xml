<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="inkant1990.com.cleanhomes">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:name=".AndroidApplication"
                 android:icon="@mipmap/ic_launcher"
                 android:roundIcon="@mipmap/ic_launcher_round"
                 android:supportsRtl="true"
                 android:theme="@style/AppTheme"
                 android:usesCleartextTraffic="true" tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".presentation.screen.student.StudentActivity"
                  android:windowSoftInputMode="adjustPan">
            <intent-filter>

                <category android:name="android.intent.category.LAUNCHER"></category>
                <action android:name="android.intent.action.MAIN"></action>
            </intent-filter>
        </activity>
        <meta-data
                android:name="io.fabric.ApiKey"
                android:value="f2336cc6279218b98db283d14a152f1447d82f8d" />
    </application>
</manifest>
